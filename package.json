  "license": "MIT",
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
    "esbuild-svelte": "^0.7.1",
    "obsidian": "latest",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "markdown-it": "^12.3.2",
    "minimatch": "^5.1.0",
    "svelte": "^3.38.2"
  }
}
